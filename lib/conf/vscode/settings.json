{
  "python.analysis.extraPaths": ["~/Content/code/lib"],
  "files.associations": {
    "*.py": "python",
    "*.lox": "plaintext",
    "*.mako": "mako",
    "*.df": "dockerfile",
    "LICENSE": "plaintext",
    "syncstream": "cpp",
    "queue": "cpp",
    "synchronized_value": "cpp",
    "execution": "cpp",
    "*.hpp": "cpp",
    "limits": "cpp",
    "stdexcept": "cpp",
    "stdbool.h": "c",
    "any": "cpp",
    "array": "cpp",
    "charconv": "cpp",
    "compare": "cpp",
    "complex": "cpp",
    "functional": "cpp",
    "ratio": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "variant": "cpp",
    "stdint.h": "c",
    "stdint-gcc.h": "c",
    "version": "cpp",
    "/usr/include/c++/*": "cpp",
    "memory.h": "c",
    "table.h": "c"
  },

  "emmet.includeLanguages": {
    "mako": "html",
    "jinja-html": "html",
    "jinja": "html",
    "pyscript": "html",
    "markdown": "md",
    "javascript": "javascript",
    "python3": "python3"
  },
  "emmet.excludeLanguages": [
    "plaintext"
  ],
  "C_Cpp.errorSquiggles": "enabled",
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": false,
  "python.defaultInterpreterPath": "~/Content/bin/venvs/defenv/bin/python3",
  "editor.fontFamily": "'Hurmit Nerd Font-', 'CaskaydiaCove Nerd Font', 'ComicShannsMono Nerd Font','.', serif",
  "editor.fontLigatures": true,
  "editor.fontWeight": "400",
  "editor.fontSize":16,
  "editor.folding": true,
  "editor.fontVariations": true,
  "C_Cpp.default.cStandard": "c23",
  "C_Cpp.default.cppStandard": "c++23",
  "python.languageServer": "Pylance",
  "python.formatting.provider": "none",
  "editor.defaultFormatter": "vscode.html-language-features",
  "prettier.tabWidth": 2,
  // "editor.tabSize": 2,
  "prettier.useTabs": false,
  "editor.indentSize": "tabSize",
  "[plaintext]": {
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[mako]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.tabSize": 4,
    "editor.indentSize": "tabSize"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[lua]": {
    "editor.defaultFormatter": "sumneko.lua",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "mkhl.shfmt",
    "editor.tabSize": 2,
    "editor.indentSize": "tabSize"
  },
  "[proto3]": {
    "editor.defaultFormatter": "zxh404.vscode-proto3"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[rust]": {
    "editor.defaultFormatter": "jinxdash.prettier-rust"
  },
  "[sql]": {
    "editor.defaultFormatter": "inferrinizzard.prettier-sql-vscode"
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  }
}
