  "python.testing.pytestEnabled": false,
  "python.defaultInterpreterPath": "~/Content/bin/py-env/bin/python3",
  "editor.fontFamily": "'Hurmit Nerd Font Propo', 'CaskaydiaCove Nerd Font Mono', 'ComicShannsMono Nerd Font Mono', serif",
